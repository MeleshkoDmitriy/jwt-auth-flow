{
  "name": "jwt-auth-flow",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run dev:web-react\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:web-react\"",
    "start:backend": "cd backend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:backend": "cd backend && npm install",
    "install:web-react": "cd web-react && npm install",
    "install:all": "npm run install:backend && npm run install:web-react",
    "start:web-react": "cd web-react && npm run dev",
    "dev:web-react": "cd web-react && npm run dev",
    "build:backend": "cd backend && npm run build",
    "clean": "rm -rf node_modules && cd backend && rm -rf node_modules && cd ../web-react && rm -rf node_modules"
  },
  "keywords": ["jwt", "auth", "expo", "react-native", "backend"],
  "author": "",
  "license": "ISC",
  "description": "JWT Authentication Flow with Backend and Mobile App",
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
